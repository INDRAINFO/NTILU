.calendar_container
{
    max-width: 1000px;
}
.calendar_container .calendar_head
{
    border: 1px solid #ccc;
    border-bottom: none;
    line-height: 60px;
    height:60px;
}
.calendar_container .calendar_head .date_filter
{
    padding: 0px 15px;
}
.calendar_container .calendar_head .form-group
{
    margin: 0px 10px 0px 0px;
}
.calendar_container .calendar_head .month_nav
{
    background-color: #eeeeee;
    border-right: 1px solid #cccccc;
    cursor: pointer;
    font-size: 20px;
    font-weight: bold;
    text-align: center;
    vertical-align: middle;
    width: 60px;
}
.calendar_container .calendar_head .next_month
{
    border-right: none;
    border-left: 1px solid #CCCCCC;
}
.calendar_container .calendar_head .month_nav:hover
{
    background-color: #DDDDDD;
}
.calendar_container .calendar_head .month_nav.disabled
{
    cursor: not-allowed;
}
.calendar_container .calendar_head .month_nav.disabled:hover
{
    background-color: #eeeeee;
}
.calendar_container .calendar_content table
{
    background-color: #CCCCCC;
    border-collapse: separate;
    border-spacing: 1px;
}
.calendar_container .calendar_content table th, .calendar_container .calendar_content table td
{
    background-color: #eeeeee;
    border: medium none;
    cursor: default;
    font-size: 1.3vw;
    height: 100%;
    padding: 2% 1%;
    text-align: center;
    vertical-align: middle;
    width: 142px;
}
.calendar_container .calendar_content table td
{
    background-color: #ffffff;
    font-size: 5vw;
    position: relative;
}
.calendar_container .calendar_content table td:hover
{
    background-color: #F5F5F5;
}
.calendar_container .calendar_content table td:empty
{
    background-color: #ffffff;
}
.calendar_container .calendar_content table td.has_event
{
    cursor: pointer;
}
.calendar_container .calendar_content table td.current_day
{
    background-color: #fff8d3;
}
.calendar_container .calendar_content table td.week_holiday
{
    color: #E02222;
}
.calendar_container .calendar_content table .th_cell
{
    word-break: break-all;
    overflow: hidden;
    max-height: 60px;
}
.calendar_container .calendar_content table .events {
    bottom: 0;
    color: #7bbcfc;
    font-size: 7px;
    left: 2px;
    position: absolute;
    right: 2px;
    text-align: right;
}
.calendar_container .calendar_content table .events .ind_event 
{
    border-radius: 50% !important;
    display: inline-block;
    height: 18px;
    line-height: 18px;
    margin-left: 2px;
    overflow: hidden;
    text-align: center;
    width: 18px;
    cursor: pointer;
}
.calendar_container .calendar_content table .events .ind_event:hover 
{
    color: #2C75BA;
}